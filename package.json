{
  "name": "tickets_app",
  "version": "1.0.0",
  "description": "This README would normally document whatever steps are necessary to get the application up and running.",
  "main": "rollup.config.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "type": "module",
  "scripts": {
    "db:new": "npx dbmate -d \"db/migrations\" -e \"DB\" new ",
    "db:up": "npx dbmate -d \"db/migrations\" -e \"DB\" up",
    "db:rollback": "npx dbmate -d \"db/migrations\" -e \"DB\" rollback",
    "db:dump": "npx dbmate -d \"db/migrations\" -e \"DB\" dump ",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "dev": "concurrently --names \"Rails,Svelte\" -c \"bgRed.bold,bgGreen\" \"rails s\" \"npm run watch\"",
    "dev-all": "concurrently --names \"Rails,Svelte,Accesos,Files\" -c \"bgRed.bold,bgGreen,bgBlue,bgWhite.bold\" \"rails s\" \"npm run watch\" \"bash -c 'cd /home/pepe/Documentos/go/accesos && go run main.go 2>/dev/null'\" \"bash -c 'cd /home/pepe/Documentos/ruby/files && npm run dev 2>/dev/null'\" \"npm run dev\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@babel/preset-env": "^7.27.2",
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-terser": "^0.4.0",
    "rollup": "^2.79.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-svelte": "^7.1.2",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-css-only": "^4.3.0",
    "concurrently": "^9.2.0",
    "dbmate": "^2.15.0"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "bootstrap": "^5.3.3",
    "font-awesome": "^4.7.0"
  }
}
